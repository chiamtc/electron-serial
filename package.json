{
  "name": "electron-serial",
  "version": "1.0.0",
  "author": "TC",
  "repository": {
    "type": "git",
    "url": "git://github.com/chiamtc/electron-serial.git"
  },
  "jest": {
    "transform": {
      "^.+\\.(jsx|js)?$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "coverageDirectory": "<rootDir>/src/app/test/",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!src/app/models/**/*.js",
      "!src/configuration/*.js",
      "!src/app/components/sharedComponents/mediaPlayer/*.js",
      "!src/app/components/sharedComponents/mediaPlayer/**/*.js",
      "!src/app/components/sharedComponents/buttons/**/*.js",
      "!src/app/components/sharedComponents/AudioPlayer/**/*.js",
      "!src/app/components/sharedComponents/google_*/*.js",
      "!src/app/components/protected/_myFiles/owner_PatientList/patient_list/*.js",
      "!src/app/components/sharedComponents/_dialogs/assignSample_dialog/*.js",
      "!src/app/test/**",
      "!src/app/*.js",
      "!src/app/components/sharedComponents/forms/_searchBar/*.js",
      "!src/app/components/protected/_myProfile/subscription_profile/card_section/newCard_dialog/*.js",
      "!webpack.config.js",
      "!gcb_slack.js",
      "!src/app/models/_interactor/*.js",
      "!index.js"
    ],
    "setupFiles": [
      "<rootDir>/src/app/test/setupTests.js"
    ],
    "modulePathIgnorePatterns": [
      "node_modules",
      "src/app/components/protected/_myFiles/owner_PatientList/patient_list/*.js",
      "src/app/components/sharedComponents/_dialogs/assignSample_dialog/*.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(svg)$": "<rootDir>/src/app/test/svgMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "electron"
    ],
    "exclude": [
      "electron/test/*.test.ts"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server",
    "eb-build": "tsc",
    "eb": "npm run eb-build && cp -a ./electron/assets ./electron-dist && electron ./electron-dist/main.js",
    "rebuild": "electron-rebuild",
    "clean": "rm -rf stage && mkdir stage",
    "build-stage": "npm run clean && webpack -p",
    "serve-stage": "PORT=9090 SERVER=DEV node index.js",
    "stage": "npm run build-stage && npm run serve-stage",
    "production": "sudo node index.js",
    "test": "jest",
    "test:coverage": "jest --coverage --silent",
    "test:example": "jest src/app/test/components/protected/_dashboard/_dashboard.test.js --collectCoverageFrom=src/app/components/protected/_dashboard/index.js --coverage",
    "test:clear": "jest -u",
    "test:watch": "jest --watch",
    "test:electron": "mocha -r ts-node/register electron/test/**/*.test.ts",
    "test:electron-coverage": "rm -rf electron/test/coverage && nyc --reporter=lcov -e .ts -X \"*.test.ts\" mocha -r ts-node/register electron/test/**/*.test.ts && nyc report && mv coverage electron/test"
  },
  "dependencies": {
    "@serialport/parser-byte-length": "^2.0.2",
    "@serialport/parser-readline": "^2.0.2",
    "@slack/client": "^4.5.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.10.4",
    "@types/promisify-node": "^0.4.0",
    "@types/serialport": "^7.0.2",
    "audio-buffer-from": "^1.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "grpc": "^1.18.0",
    "lodash": "^4.17.11",
    "node-cmd": "^3.0.0",
    "pcm-convert": "^1.6.5",
    "promisify-node": "^0.5.0",
    "serialport": "^7.1.4",
    "sox-stream": "^2.0.3",
    "strip": "^3.0.0",
    "typescript": "^3.3.3333",
    "usb": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/parser": "^7.3.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.1.3",
    "@babel/plugin-transform-for-of": "^7.0.0",
    "@babel/plugin-transform-shorthand-properties": "^7.0.0",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/plugin-transform-template-literals": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@date-io/moment": "^1.1.0",
    "@m3dicine/core": "^1.0.22",
    "@m3dicine/icons": "^1.0.3",
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.24",
    "@types/chai": "^4.1.7",
    "autosuggest-highlight": "^3.1.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "bootstrap": "^4.0.0-beta",
    "chai": "^4.2.0",
    "convert-units": "^2.3.4",
    "copy-webpack-plugin": "^4.2.1",
    "css-loader": "^0.28.7",
    "electron": "^4.1.1",
    "electron-rebuild": "^1.8.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "jquery": "^3.2.1",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "jss": "^9.0.0",
    "lodash": "^4.17.4",
    "material-ui-pickers": "^2.2.4",
    "mathjs": "^7.5.1",
    "mic": "^2.1.2",
    "mocha": "^6.0.2",
    "moment": "^2.19.3",
    "node-audiorecorder": "^1.4.0",
    "node-record-lpcm16": "^0.3.1",
    "nyc": "^13.3.0",
    "parallelshell": "^3.0.2",
    "popper": "^1.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.6.0",
    "react-autosuggest": "^9.4.2",
    "react-chartjs-2": "^2.6.4",
    "react-currency-formatter": "^1.1.0",
    "react-dom": "^16.6.0",
    "react-geocode": "^0.1.2",
    "react-google-maps": "^9.4.1",
    "react-images-upload": "^1.2.0",
    "react-places-autocomplete": "^7.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-sparklines": "^1.7.0",
    "react-stripe-elements": "^2.0.1",
    "react-svg-loader": "^1.1.1",
    "react-tabs": "^2.2.2",
    "react-test-renderer": "^16.4.2",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-promise": "^0.5.3",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "rxfire": "^3.2.2",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "sass-loader": "^6.0.6",
    "sinon": "^6.1.5",
    "style-loader": "^0.19.0",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1",
    "ts-node": "^8.0.3",
    "validator": "^10.8.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
